{
  "$schema": "https://zed.dev/schema/themes/v0.1.0.json",
  "name": "Trigger Dark",
  "author": "Converted from VSCode Theme",
  "themes": [
    {
      "name": "Trigger Dark",
      "appearance": "dark",
      "style": {
        "background": "#121317",
        "surface.background": "#121317",
        "elevated_surface.background": "#121317",
        "element.background": "#383a49",
        "element.hover": "#383a49cc",
        "element.selected": "#383a49cc",
        "text": "#878C99",
        "text.muted": "#656974",
        "status_bar.background": "#121317",
        "title_bar.background": "#121317",
        "tab_bar.background": "#121317",
        "tab.inactive_background": "#121317",
        "tab.active_background": "#1A1B1F",
