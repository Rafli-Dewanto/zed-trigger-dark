        "editor.active_line_number": "#CFD6E4",
        "editor.wrap_guide": "#1A1B1F",
        "editor.active_wrap_guide": "#1A1B1F",
        "terminal.background": "#121317",
        "terminal.foreground": "#878C99",
        "terminal.ansi.red": "#F85149",
        "terminal.ansi.green": "#A8FF53",
        "terminal.ansi.yellow": "#F9D849",
        "terminal.ansi.blue": "#479DEC",
        "terminal.ansi.magenta": "#D886FF",
        "terminal.ansi.cyan": "#618FE7",
        "terminal.ansi.white": "#CFD6E4",
        "terminal.ansi.black": "#121317",
        "panel.background": "#121317",
